{
	"name": "l13-diff",
	"displayName": "L13 Diff",
	"description": "Compare Two Folders in Visual Studio Code",
	"version": "0.16.0",
	"publisher": "L13RARY",
	"license": "SEE LICENCE IN LICENCE.md",
	"preview": true,
	"homepage": "https://github.com/l13/vscode-diff/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/l13/vscode-diff.git"
	},
	"bugs": {
		"url": "https://github.com/l13/vscode-diff/issues"
	},
	"icon": "images/icon.png",
	"galleryBanner": {
		"color": "#232323",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"compare",
		"diff",
		"folder",
		"file",
		"workspace"
	],
	"activationEvents": [
		"onCommand:l13Diff.show",
		"onCommand:l13Diff.open",
		"onCommand:l13Diff.clearHistory",
		"onCommand:l13Diff.clearFavorites",
		"onView:diffExplorerFavorites",
		"onWebviewPanel:l13Diff"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "l13-diff",
					"title": "L13 Diff",
					"icon": "images/favorites.svg"
				}
			]
		},
		"views": {
			"l13-diff": [
				{
					"id": "diffExplorerFavorites",
					"name": "Favorites"
				}
			]
		},
		"commands": [
			{
				"command": "l13Diff.show",
				"title": "L13 Diff"
			},
			{
				"command": "l13Diff.open",
				"title": "Open in L13 Diff"
			},
			{
				"command": "l13Diff.clearHistory",
				"title": "L13 Diff: Clear History"
			},
			{
				"command": "l13Diff.openFavorite",
				"title": "Open"
			},
			{
				"command": "l13Diff.addToFavorites",
				"title": "Add to favorites",
				"icon": {
					"light": "images/favorite-light.svg",
					"dark": "images/favorite-dark.svg"
				}
			},
			{
				"command": "l13Diff.removeFavorite",
				"title": "Remove"
			},
			{
				"command": "l13Diff.clearFavorites",
				"title": "L13 Diff: Remove all favorites"
			}
		],
		"keybindings": [
			{
				"mac": "meta+l meta+l",
				"key": "ctrl+l ctrl+l",
				"command": "l13Diff.show"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "l13Diff.open",
					"group": "3_compare@1"
				}
			],
			"view/item/context": [
				{
					"command": "l13Diff.openFavorite",
					"when": "view == diffExplorerFavorites && viewItem == favorite"
				},
				{
					"command": "l13Diff.removeFavorite",
					"when": "view == diffExplorerFavorites && viewItem == favorite"
				}
			],
			"editor/title": [
				{
					"when": "l13DiffFocus",
					"command": "l13Diff.addToFavorites",
					"group": "navigation"
				}
			]
		},
		"configuration": {
			"type": "object",
			"title": "L13 Diff",
			"properties": {
				"l13Diff.maxRecentlyUsed": {
					"type": "number",
					"default": 10,
					"description": "How many recently used entries should be visible in the menu?"
				},
				"l13Diff.openToSide": {
					"type": "boolean",
					"default": false,
					"description": "Should diff or file open to side?"
				},
				"l13Diff.ignore": {
					"type": "array",
					"default": [
						".DS_Store",
						".git",
						".svn",
						"CVS",
						"node_modules"
					],
					"description": "Indicates folders or files to be ignored, like \".git\", \".svn\", \"CVS\", \"node_modules\", etc."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "gulp build",
		"watch": "gulp watch",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"del": "^4.1.1",
		"gulp": "^4.0.2",
		"gulp-concat": "^2.6.1",
		"gulp-eol": "^0.2.0",
		"gulp-sass": "^4.0.2",
		"gulp-typescript": "^5.0.1",
		"rollup": "^1.12.3",
		"rollup-plugin-typescript": "^1.0.1",
		"tslint": "^5.16.0",
		"typescript": "^3.4.5",
		"vscode": "^1.1.34"
	}
}
